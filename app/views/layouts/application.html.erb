
<!DOCTYPE html>
<html>
  <head>
    <title>My Map Run</title>
    <%= csrf_meta_tags %>
    <script>
      window.gak = "<%= ENV['google_api_key'] %>"
      window.gsk = "<%= ENV['google_static_api_key'] %>"
    </script>
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Open+Sans" />
    <script src= "https://maps.googleapis.com/maps/api/js?key=<%= ENV['google_api_key']%>&libraries=geometry,places"
    ></script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
